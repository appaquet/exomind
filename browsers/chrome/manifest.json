{
    "manifest_version": 2,
    "name": "Exomind",
    "description": "Import into Exomind",
    "version": "0.3",
    "background": {
        "scripts": [
            "event.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "app@exomind.io",
            "strict_min_version": "42.0"
        }
    },
    "permissions": [
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}